<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:startTakePhotoView="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:onClick="toCameraClicked"
        android:text="start" />

    <com.ui.jerry.StartTakePhotoView
        android:id="@+id/startTakePhotoView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        startTakePhotoView:src="@drawable/topic_icon_camera" />

    <ImageView
        android:id="@+id/image"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_marginTop="200dp"
        android:scaleType="fitXY" />


